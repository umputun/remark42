{
  "name": "remark-ui",
  "version": "0.1.0",
  "scripts": {
    "build": "webpack --mode production && node postbuild.js",
    "start": "webpack-dev-server --mode development",
    "check": "tsc -p tsconfig.json --noEmit --skipLibCheck",
    "lint": "eslint --max-warnings=0 --ext=.ts,.tsx,.js,.jsx .",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "prettier": "prettier --write \"./**/*.{js,jsx,ts,tsx,scss}\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "./node_modules/.bin/lint-staged",
      "post-commit": "git update-index --again",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "./**/*.{ts,tsx,js,jsx}": [
      "eslint --fix --max-warnings=0",
      "git add"
    ],
    "./**/*.scss": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/preset-env": "^7.5.4",
    "@babel/preset-react": "^7.0.0",
    "@types/cheerio": "^0.22.12",
    "@types/core-js": "^2.5.2",
    "@types/fetch-mock": "^7.3.1",
    "@types/jest": "^24.0.15",
    "@types/lodash": "^4.14.135",
    "@types/node": "^12.6.3",
    "@types/redux-mock-store": "^1.0.1",
    "@typescript-eslint/eslint-plugin": "^1.12.0",
    "@typescript-eslint/parser": "^1.12.0",
    "autoprefixer": "^9.6.1",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^3.0.0",
    "document-register-element": "^1.13.2",
    "enzyme": "^3.10.0",
    "enzyme-adapter-preact-pure": "^2.0.0",
    "es-check": "^5.0.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.2",
    "fetch-mock": "^7.3.6",
    "file-loader": "^4.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.0",
    "jest": "^24.8.0",
    "jest-enzyme": "^7.0.2",
    "jest-extended": "^0.11.2",
    "jest-localstorage-mock": "^2.4.0",
    "lint-staged": "^9.2.0",
    "lodash": "^4.17.11",
    "mini-css-extract-plugin": "^0.7.0",
    "node-fetch": "^2.6.0",
    "npm-run-all": "^4.1.5",
    "postcss-calc": "^7.0.1",
    "postcss-csso": "^3.0.0",
    "postcss-for": "^2.1.1",
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^4.1.2",
    "postcss-simple-vars": "^5.0.2",
    "postcss-url": "^8.0.0",
    "postcss-wrap": "0.0.4",
    "preact-context": "^1.1.3",
    "prettier": "^1.18.2",
    "redux-mock-store": "^1.5.3",
    "style-loader": "^0.23.1",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.4",
    "typescript": "^3.5.3",
    "webpack": "^4.35.3",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2",
    "whatwg-fetch": "^3.0.0"
  },
  "dependencies": {
    "@github/markdown-toolbar-element": "^1.0.4",
    "@webcomponents/custom-elements": "^1.2.4",
    "bem-react-helper": "^1.1.2",
    "core-js": "^3.1.4",
    "focus-visible": "^5.0.2",
    "preact": "^8.4.2",
    "preact-redux": "^2.0.3",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0"
  },
  "eslintIgnore": [
    "public"
  ],
  "engines": {
    "node": ">=8"
  }
}
